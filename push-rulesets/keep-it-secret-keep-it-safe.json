{
  "name": "keep-it-secret-keep-it-safe",
  "target": "push",
  "source": "repo-rules-demo/dev-repo1",
  "enforcement": "active",
  "conditions": null,
  "rules": [
    {
      "type": "file_path_restriction",
      "parameters": {
        "restricted_file_paths": [
          ".github/workflows/**/*"
        ]
      }
    },
    {
      "type": "file_extension_restriction",
      "parameters": {
        "restricted_file_extensions": [
          "*.jar",
          "*.bin"
        ]
      }
    },
    {
      "type": "max_file_size",
      "parameters": {
        "max_file_size": 10
      }
    }
  ],
  "bypass_actors": [
    {
      "actor_id": 1,
      "actor_type": "OrganizationAdmin",
      "bypass_mode": "always"
    }
  ]
}
